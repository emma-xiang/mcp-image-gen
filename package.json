{
  "name": "flux-schnell-server",
  "version": "0.1.0",
  "description": "MCP for generating image via replicate flux schnell",
  "private": true,
  "bin": {
    "flux-schnell-server": "./build/index.js"
  },
  "files": [
    "build"
  ],
